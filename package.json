  "license": "(Unlicense OR MIT)",
  "bugs": {
    "url": "https://github.com/minagawah/flight-pack/issues"
  },
  "devDependencies": {
    "@wasm-tool/wasm-pack-plugin": "^1.1.0",
    "copy-webpack-plugin": "^5.0.3",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.0",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.3",
    "webpack-dev-server": "^3.7.1"
  }
}
