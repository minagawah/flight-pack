{
  "name": "flight-pack",
  "version": "0.1.0",
  "private": true,
  "description": "Flight simulation using wasm-pack",
  "main": "public/index.html",
  "scripts": {
    "clippy": "cd src && cargo fmt && cargo clippy",
    "prettier": "prettier --write \"*.{js,md,json}\" \"js/*.js\" \"static/*.{css,html,js}\"",
    "build": "rimraf dist pkg && webpack",
    "dev": "webpack-dev-server",
    "test": "cargo test && wasm-pack test --headless --firefox"
